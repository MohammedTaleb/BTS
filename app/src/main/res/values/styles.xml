<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionMenuTextColor">@color/textsec</item>
        <item name="android:actionMenuTextColor">@color/textsec</item>
        <item name="android:textColorPrimary">@color/textsec</item>
        <item name="android:titleTextStyle">@color/white</item>
        <item name="android:title">@color/white</item>



    </style>
    <style name="Progressbar.White" parent="AppTheme">
        <item name="colorControlActivated">@color/white</item>
    </style>



</resources>
